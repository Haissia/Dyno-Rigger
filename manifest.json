{
    "manifest_version": 3,
    "name": "Dyno Coinflip Rigger",
    "version": "1.0",
    "description": "Rig the Dyno's bot coinflip.",
    "permissions": [
      "cookies",
      "activeTab"
    ],
    "host_permissions": [
      "https://www.roblox.com/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }
  